<Window x:Class="Owner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Owner"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock FontFamily="Arial" FontSize="30" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,10,0,0">Lijst van admins</TextBlock>
        <TextBox Margin="0,50,0,0" VerticalAlignment="Top" Width="300"></TextBox>
        <ListBox Margin="0,80,0,0" ScrollViewer.VerticalScrollBarVisibility="Visible">
            <ListBoxItem></ListBoxItem>
        </ListBox>
    </Grid>
</Window>
